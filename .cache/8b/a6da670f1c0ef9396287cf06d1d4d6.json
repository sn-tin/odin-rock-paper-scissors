ke-dasharray='3%2c 15' stroke-dashoffset='0' stroke-linecap='square'/%3e%3c/svg%3e\");\n    background-color: $white;\n}\n\n.svt-card {\n    background-color: $serenity;\n    @extend .carat-card;\n}\n\n.score {\n    display: flex;\n    flex-direction: row;\n    // justify-content: space-evenly;\n    align-items: center;\n    // width: 200px;\n    margin: 5px auto;\n    background-color: $gray;\n    border: 3px solid $black;\n    border-radius: 20px;\n    box-shadow: inset -3px 3px hsla(0, 0%, 0%, 0.25);\n}\n\n.score-number {\n    font-size: clamp(0.5rem , 2.5vw, 1rem);\n    margin-bottom: 0;\n    width: 75px;\n}\n\n.score-label {\n    @extend .score-number;\n    background-color: $serenity;\n    border-radius: 20px;\n    border-right: 3px solid $black;\n}\n\n.carat-img, .svt-img {\n    border-radius: 50%;\n    border: 3px solid $black;\n    margin: 10px auto;\n}\n\n.buttons {\n    display: flex;\n    flex-direction: column;\n    background-color: $gray;\n    border-radius: 20px;\n    border: 3px solid $black;\n    box-shadow: inset 0 -5px hsla(0, 0%, 0%, 0.25);\n}\n\n.carat-name, .svt-name {\n    font-family: $small-font;\n    font-size: clamp(0.5rem , 2.5vw, 1rem);\n    background-color: $serenity;\n    border-radius: 20px 20px 0 0;\n    border-bottom: 3px solid $black;\n    padding: 3px;\n}\n\n.play-buttons {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-around;\n    gap: 10px;\n    padding: 20px;\n}\n\n.btn-img {\n    width: 50px;\n    height: 50px;\n    box-shadow: inset 0 -5px hsla(0, 0%, 0%, 0.25);\n    border-radius: 5px;\n    border: 3px solid $black;\n    padding: 10px;\n}"},"lineCount":null}},"error":null,"hash":"1fbb0eb5778492b16618e923848ad7f0","cacheData":{"env":{}}}