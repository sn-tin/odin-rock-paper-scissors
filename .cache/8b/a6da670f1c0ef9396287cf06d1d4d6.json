al":{"line":176,"column":0},"generated":{"line":230,"column":0}},{"source":"sass/_cards.scss","name":null,"original":{"line":177,"column":4},"generated":{"line":231,"column":2}},{"source":"sass/_cards.scss","name":null,"original":{"line":180,"column":0},"generated":{"line":234,"column":0}}],"sources":{"sass/_variables.scss":"/* Colors */\n$light-yellow: hsl(47, 100%, 94%);\n$rose-quartz: hsl(1, 74%, 88%);\n$serenity: hsl(218, 40%, 69%);\n\n$black: hsl(0, 0%, 0%);\n$white: hsl(0, 0%, 100%);\n$gray: hsl(0, 18%, 93%);\n\n/* Fonts */\n$big-font: 'Press Start 2P';\n$small-font: 'Changa';\n\n/* Font Weight */\n$semi-bold: 600;\n$bold: 700;","sass/_base.scss":"* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\nbody {\n    background-color: $light-yellow;\n    font-size: 16px;\n}\n\nh1, h2 {\n    font-family: $big-font;\n}\n\nh3, p {\n    font-family: $small-font;\n}\n\nmain {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}","sass/_banner-header.scss":"/* Banner */\n.banner {\n    width: 100%;\n    background-color: $rose-quartz;\n    p {\n        font-size: clamp(0.375rem, 2.5vw, 1.2rem);\n        text-align: center;\n        margin: 15px auto;\n    }\n}\n\n/* End of Banner */\n\n/* Container */\n.container {\n    text-align: center;\n    margin: 50px auto;\n}\n/* End of Container */\n\n/* Header Text */\n.header-text {\n    h1 {\n        width: 576px;\n        margin: 0 auto;\n        font-size: clamp(1.125rem, 2.5vw, 3rem);\n        text-transform: uppercase;\n        color: $white;\n        -webkit-text-stroke-width: 2px;\n        -webkit-text-stroke-color: black;\n    }\n    p {\n        font-size: clamp(0.688rem , 2.5vw, 1.875rem);\n        font-weight: $bold;\n        color: $white;\n        -webkit-text-stroke-width: 1px;\n        -webkit-text-stroke-color: black;\n        margin: 10px 0;\n    }\n}\n/* End of Header Text */\n\n/* Result */\n\n.result {\n    width: 620px;\n    padding: 15px;\n    margin: 50px auto;\n    background-color: $gray;\n    border-radius: 20px;\n    border: 3px solid $black;\n    h3 {\n        font-size: clamp(0.875rem, 2.5vw, 2.188rem);\n        margin-bottom: 0;\n    }\n}","sass/_cards.scss":".players-card {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: center;\n    gap: 100px;\n    margin-top: 30px;\n\n    h2 {\n        color: $white;\n        -webkit-text-stroke-width: 2px;\n        -webkit-text-stroke-color: black;\n        text-shadow: 0 5px hsla(0, 0%, 0%, 0.25);\n    }\n}\n\n/* CARAT Card */\n\n.carat-card {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    width: 300px;\n    padding: 15px;\n    background-color: $rose-quartz;\n    border-radius: 15px;\n    border: 5px solid $black;\n    box-shadow: -15px 15px hsla(0, 0%, 0%, 0.25);\n}\n\n.card-content {\n    width: 258px;\n    padding: 25px;\n    /* generated dashed border */\n    background-image: url(\"data:image/svg+xml,%3csvg width='100%25' height='100%25' xmlns='http://www.w3.org/2000/svg'%3e%3crect width='100%25' height='100%25' fill='none' stroke='black' stroke-width='10' stroke-dasharray='3%2c 15' stroke-dashoffset='0' stroke-linecap='square'/%3e%3c/svg%3e\");\n    background-color: $white;\n}\n\n.score {\n    display: flex;\n    flex-direction: row;\n    // justify-content: space-evenly;\n    align-items: center;\n    // width: 200px;\n    margin: 5px auto;\n    background-color: $gray;\n    border: 3px solid $black;\n    border-radius: 20px;\n    box-shadow: inset -3px 3px hsla(0, 0%, 0%, 0.25);\n}\n\n.score-number {\n    font-size: clamp(0.5rem , 2.5vw, 1rem);\n    margin-bottom: 0;\n    width: 75px;\n}\n\n.score-label {\n    @extend .score-number;\n    background-color: $serenity;\n    border-radius: 20px;\n    border-right: 3px solid $black;\n}\n\n.carat-img {\n    border-radius: 50%;\n    border: 3px solid $black;\n    margin: 10px auto;\n}\n\n.buttons {\n    display: flex;\n    flex-direction: column;\n    background-color: $gray;\n    border-radius: 20px;\n    border: 3px solid $black;\n    box-shadow: inset 0 -5px hsla(0, 0%, 0%, 0.25);\n    margin: 5px 0 0;\n}\n\n.carat-name {\n    font-family: $small-font;\n    font-size: clamp(0.5rem , 2.5vw, 1rem);\n    background-color: $serenity;\n    border-radius: 15px 15px 0 0;\n    border-bottom: 3px solid $black;\n    padding: 3px;\n}\n\n.play-buttons {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-around;\n    gap: 10px;\n    padding: 20px;\n}\n\n.btn-img {\n    width: 50px;\n    height: 50px;\n    box-shadow: inset 0 -5px hsla(0, 0%, 0%, 0.25);\n    border-radius: 10px;\n    border: 3px solid $black;\n    padding: 10px;\n}\n\n/* End of CARAT Card */\n\n/* SEVENTEEN Card */\n\n// Flex svt card and reload button\n\n.card-btn-flex {\n    display: flex;\n    flex-wrap: row wrap;\n    align-items: center;\n    gap: 30px;\n}\n\n// Reload button attributes\n\n.reload {\n    border-radius: 10px;\n    background-color: $white;\n    border: 3px solid $black;\n    box-shadow: 5px 5px hsla(0, 0%, 0%, 0.25);\n}\n\n.reload:hover {\n    background-color: $serenity;\n}\n\n.reload-btn {\n    width: 50px;\n    padding: 13px;\n    border: none;\n    background-color: transparent;\n    transition: all 1s ease-in;\n}\n\n// To add using JavaScript\n\n.reload-btn-animation {\n    // animation-name: rotate;\n    // animation-duration: 1s;\n    // animation-iteration-count: infinite;\n    // animation-timing-function: 1;\n    // animation-play-state: running;\n    transform: rotate(360deg);\n    transition: all 0.5s ease-in;\n}\n\n// @keyframes rotate {\n\n//     from {transform: rotate(0deg)}\n//     to {transform: rotate(360deg)}\n    \n// }\n\n.svt-card {\n    background-color: $serenity;\n    @extend .carat-card;\n\n    .score-label {\n        background-color: $rose-quartz;\n    }\n}\n\n.svt-name {\n    background-color: $rose-quartz;\n    @extend .carat-name;\n}\n\n.svt-img {\n    width: 84px;\n    @extend .carat-img;\n}\n/* End of SEVENTEEN Card */"},"lineCount":null}},"error":null,"hash":"8db062331d34946670e819379bdca07e","cacheData":{"env":{}}}